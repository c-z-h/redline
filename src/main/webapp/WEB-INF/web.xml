<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <servlet-name>ResidentQuestionnaire</servlet-name>
    <servlet-class>questionnaire.resident.ResidentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResidentQuestionnaire</servlet-name>
    <url-pattern>/residentquestionnaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MonitorQuestionnaire</servlet-name>
    <servlet-class>questionnaire.monitor.MonitorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MonitorQuestionnaire</servlet-name>
    <url-pattern>/monitorquestionnaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ExpertQuestionnaire</servlet-name>
    <servlet-class>questionnaire.expert.ExpertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExpertQuestionnaire</servlet-name>
    <url-pattern>/expertquestionnaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Weighting</servlet-name>
    <servlet-class>weighting.WeightingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Weighting</servlet-name>
    <url-pattern>/Weighting</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Segment</servlet-name>
    <servlet-class>segment.SegmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Segment</servlet-name>
    <url-pattern>/Segment</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>/WEB-INF/applicationContext-*.xml,classpath*:*/applicationContext-*.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>/index.html</welcome-file>
  </welcome-file-list>
</web-app>